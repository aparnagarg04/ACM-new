{
	"name": "acm-usict",
	"version": "1.2.0",
	"description": "User portal for GGSIP University USS ACM Student Chapter",
	"main": "index.js",
	"keywords": [
		"react",
		"reactjs",
		"argon",
		"argon-react",
		"dashboard",
		"dashboard-react",
		"argon-dashboard",
		"argon-dashboard-react"
	],
	"license": "MIT",
	"homepage": "http://usict.acm.org/dashboard/user-portal",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
		"compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
		"minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
		"map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
		"build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"dependencies": {
		"@ckeditor/ckeditor5-build-classic": "^34.2.0",
		"@ckeditor/ckeditor5-build-decoupled-document": "^34.2.0",
		"@ckeditor/ckeditor5-react": "^5.0.2",
		"@ckeditor/ckeditor5-upload": "^34.2.0",
		"@emotion/react": "^11.9.3",
		"@emotion/styled": "^11.9.3",
		"@fortawesome/fontawesome-free": "6.1.1",
		"@mui/material": "^5.9.1",
		"@popperjs/core": "^2.11.5",
		"@reduxjs/toolkit": "^1.8.3",
		"axios": "^0.27.2",
		"bootstrap": "^4.6.2",
		"buffer": "^6.0.3",
		"chart.js": "3.8.0",
		"ckeditor5-custom-build": "file:./ckeditor5",
		"classnames": "2.2.6",
		"moment": "2.29.4",
		"node-sass": "7.0.0",
		"node-sass-package-importer": "5.3.2",
		"nouislider": "15.6.0",
		"react": "^18.2.0",
		"react-calendar": "^4.0.0",
		"react-chartjs-2": "4.3.1",
		"react-copy-to-clipboard": "5.1.0",
		"react-datetime": "3.1.1",
		"react-dom": "18.2.0",
		"react-html-parser": "^2.0.2",
		"react-moment": "^1.1.2",
		"react-moment-countdown": "^0.4.2",
		"react-moment-proptypes": "^1.8.1",
		"react-redux": "^8.0.2",
		"react-router": "^5.3.3",
		"react-router-dom": "^5.3.3",
		"react-scripts": "5.0.1",
		"reactstrap": "^8.10.1",
		"styled-components": "^5.3.6",
		"webpack-dev-server": "^4.9.3"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.13",
		"gulp": "^4.0.2",
		"gulp-append-prepend": "1.0.9",
		"postcss": "^8.4.19",
		"webpack": "^5.73.0"
	},
	"optionalDependencies": {
		"eslint-plugin-flowtype": "8.0.3",
		"jquery": "3.6.0",
		"typescript": "4.7.4"
	}
}
